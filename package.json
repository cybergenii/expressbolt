{
  "name": "expressbolt",
  "version": "1.0.2",
  "description": "Crud helper using mongoose to make creation of api fast",
  "keywords": [
    "mongoose",
    "crud",
    "api",
    "helper",
    "typescript",
    "express"
  ],
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts ",
  "dependencies": {
    "@types/express": "^4.17.21",
    "express": "^4.18.2",
    "http-status": "^1.7.3",
    "mongoose": "^8.1.1",
    "typescript": "^5.3.3",
    "undici-types": "^5.26.5"
  },
  "scripts": {
    "pretest": "npm run build",
    "check:tsd": "npm run build:dts && tsd",
    "build:ts": "node ./node_modules/typescript/bin/tsc",
    "build:dts": "npm run build:ts",
    "build": "npm run build:dts",
    "check:lint": "eslint -v && eslint --ext '.js,.ts' --max-warnings=0 src test && npm run build:dts && npm run check:tsd",
    "format": "eslint --ext '.js,.ts' src test --fix",
    "check:coverage": "nyc --check-coverage npm run check:node"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.11.10"
  }
}